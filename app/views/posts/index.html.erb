<h1>FirstAPP</h1>
<%= form_with url: "/posts", method: :post, id: "form" do |form|%>
  <% form.text_field :content%>
  <%= form.submit '投稿する', id: "submit"%>
<% end %>

  <div class="post">
    <div class="post-date">
      tweeted date：<%= post.created_at %>
    </div>
    <div class="post-content">
      <%= post.content %>
    </div>
  </div>
<% end %>